#include<stdio.h>
int main ()
{
    
        int num[5],i,sum=0;
        num[0]=10;
        num[1]=20;
        num[2]=30;
        num[3]=40;
        num[4]=50;

        
        for (i=1;i<5;i++)
        {
            
            sum= sum+num[i];
            
        }
        printf("%d is resulr\n",sum);
        
    
}
