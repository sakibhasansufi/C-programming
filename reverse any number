#include<stdio.h>
int main ()
{
    while (1)
    {
        int m,r,temp,sum=0;
        printf("Enter number \n",m);
        scanf("%d",&m);
        temp=m;
        while(temp!=0)
        {
            r=temp%10;
            sum=sum*10+r;
            temp=temp/10;
        }
        printf("The result is %d\n",sum);
    }
}
