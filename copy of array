#include<stdio.h>
int main ()
{
    int array1[5]={1,2,3,4,5};
    int i,array2[5];
    printf("Array 1 :");
    for (i=0;i<5;i++)
    {
        printf("%d  ",array1[i]);
    }
    for (i=0;i<5;i++)
    {
        array2[i]=array1[i];
    }
    printf("\nArray 2 :");
    for (i=0;i<5;i++)
    {
        printf("%d  ",array2[i]);
    }
}
