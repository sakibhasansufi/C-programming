#include<stdio.h>
int main ()
{
    int num[200],n,i,sum=0;
    printf("How many number\n");
    scanf("%d",&n);
    printf("Enter numbers");
    for (i=0;i<n;i++)
    {
        scanf("%d",&num[i]);
    }
    int min= num[0];
    int max= num[0];
    for (i=1;i<n;i++)
    {
        if (min>num[i])
        {
            min=num[i];
        }
        else if (max<num[i])
        {
            max=num[i];
        }
    }
    printf("maximum number is %d\n",max);
    printf("maximum number is %d",min);

}
