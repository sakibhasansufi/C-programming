#include<stdio.h>
int main ()
{
    int rp[3][3];
    int i,j;
    for(i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            printf("rp[%d][%d]= ",i,j);
            scanf("%d",&rp[i][j]);
        }
        printf("\n");
    }
    for(i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            printf("%d  ",rp[i][j]);
        }
        printf("\n");
    }
    
}
