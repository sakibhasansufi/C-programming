#include<stdio.h>
int main ()

{
  while(1){
    int num1,num2,lcm,gcd,n1,n2,rem;
    printf("Enter two numbers ",num1,num2);
    scanf("%d %d",&num1,&num2);
    n1=num1;
    n2=num2;
    while(n2!=0){
        rem=n1%n2;
        n1=n2;
        n2=rem;
    }
    gcd=n1;
    lcm=(num1*num2)/gcd;
    printf("gcd is %d\n",gcd);
    printf("lcm is %d\n",lcm);
    

  
  }
}
